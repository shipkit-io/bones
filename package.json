		"remark-frontmatter": "^5.0.0",
		"remark-mdx-frontmatter": "^4.0.0",
		"sonner": "1.7.1",
		"superjson": "2.2.2",
		"tailwind-merge": "2.6.0",
		"tailwindcss-animate": "1.0.7",
		"usehooks-ts": "^3.1.0",
		"zod": "3.24.1",
		"zsa": "0.6.0",
		"zustand": "5.0.2",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@next/eslint-plugin-next": "15.1.2",
		"@next/mdx": "15.1.2",
		"@tailwindcss/typography": "0.5.15",
		"@types/node": "22.10.2",
		"@types/react": "19.0.2",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "10.4.20",
		"eslint": "^8.56.0",
		"eslint-config-next": "15.1.6",
		"postcss": "8.4.49",
		"tailwindcss": "3.4.17",
		"typescript": "^5"
	},
	"ct3aMetadata": {
		"initVersion": "7.37.0"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md,yml}": [
			"prettier --write"
		]
	},
	"volta": {
		"node": "22.11.0"
	},
	"pnpm": {
		"overrides": {
			"react": "19.0.0",
			"react-dom": "19.0.0"
		}
	}
}
